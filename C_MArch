#include <stdio.h>

void sum(int *arr, int size, int *sum);
int  main(){
	int size = 2;
	int array[size];
	array[0] = 5;
	array[1] = 6;
	array[2] = -1;
	int result = 0; 
	printf("%d\n",result);
	sum(array,size,&result);
}

void sum(int *arr, int size, int *sum){
	for(int i=0;i<=size;i++)
	{
		*sum += arr[i];
		  
	}
	printf("%d",*sum);

}
